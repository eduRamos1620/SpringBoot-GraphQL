#Query - solo para hacer consultas
type Query{
    findStudentById(studentId: String) : Student,
    findAllStudents : [Student]
}

type Mutation{
    createStudents(inputStudent : InputStudent) : Student,
    deleteStudentById(studentId : String) : String
}

type Student {
    id : ID,
    name : String,
    lastname : String,
    age : Int
}

input InputStudent{
    id : ID,
    name : String,
    lastname : String,
    age : Int,
    courseId : String!
}